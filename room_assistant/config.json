{
  "name": "room-assistant",
  "version": "2.9.0",
  "slug": "room_assistant",
  "description": "Presence tracking and more for automation on the room-level",
  "url": "https://www.room-assistant.io",
  "stage": "stable",
  "advanced": false,
  "startup": "application",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "image": "mkerix/room-assistant-{arch}",
  "boot": "auto",
  "services": [
    "mqtt:want"
  ],
  "apparmor": false,
  "host_network": true,
  "host_dbus": true,
  "gpio": true,
  "privileged": [
    "NET_ADMIN"
  ],
  "ports": {
    "6415/tcp": null,
    "6425/udp": 6425
  },
  "ports_description": {
    "6415/tcp": "HTTP API",
    "6425/udp": "Internal communication between instances"
  },
  "options": {
    "global": {
      "instanceName": "changeme",
      "integrations": [
        "homeAssistant"
      ]
    }
  },
  "schema": false
}