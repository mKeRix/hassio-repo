{
  "name": "room-assistant",
  "version": "latest",
  "slug": "room_assistant",
  "description": "Presence tracking and more for automation on the room-level",
  "url": "https://www.room-assistant.io",
  "stage": "stable",
  "advanced": false,
  "startup": "application",
  "watchdog": "http://[HOST]:[PORT:6415]/status",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "image": "wallacemreis/room-assistant-{arch}",
  "boot": "auto",
  "services": [
    "mqtt:want"
  ],
  "apparmor": false,
  "host_network": true,
  "host_dbus": true,
  "gpio": true,
  "privileged": [
    "NET_ADMIN"
  ],
  "ports": {
    "6415/tcp": 6415,
    "6425/udp": 6425
  },
  "ports_description": {
    "6415/tcp": "HTTP API",
    "6425/udp": "Internal communication between instances"
  },
  "map": [
    "config:rw"
  ],
  "schema": false
}
